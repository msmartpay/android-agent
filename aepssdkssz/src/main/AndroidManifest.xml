<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />
    <queries>
        <package android:name="com.mantra.rdservice" />
        <package android:name="com.acpl.registersdk" />
        <package android:name="com.tatvik.bio.tmf20" />
        <package android:name="com.secugen.rdservice" />
        <package android:name="com.evolute.rdservice" />
        <package android:name="com.scl.rdservice" />

        <intent>
            <action android:name="android.intent.action.SEND" />
            <data android:mimeType="image/jpeg" />
        </intent>
    </queries>
    <application
        android:hardwareAccelerated="true"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/AppTheme"
        tools:node="merge"
        tools:targetApi="n">
        <activity
            android:name=".SSZAePSHomeActivity"
            android:configChanges="screenSize|orientation"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false"/>
        <activity
            android:name=".FingpayAePSHomeActivity"
            android:configChanges="screenSize|orientation"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false"/>
        <activity
            android:name=".paysprint.PSAePSHomeActivity"
            android:configChanges="screenSize|orientation"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false"/>
        <activity
            android:name=".paysprint.PaySprintOnboardActivity"
            android:configChanges="screenSize|orientation"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false"/>
    </application>

</manifest>